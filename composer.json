{
    "name": "abhimanyu003/conversion",
    "description": "Laravel Unit Conversion",
     "keywords": [
         "laravel", "unit", "measurements", "conversion"
     ],
    "homepage":    "https://github.com/abhimanyu003/conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Abhimanyu Sharma",
            "email": "abhimanyusharma003@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*|~5.0"
    },
    "autoload": {
        "psr-0": {
            "Abhimanyu003\\Conversion": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Abhimanyu003\\Conversion\\ConversionServiceProvider"
            ],
            "aliases": {
                "Conversion": "Abhimanyu003\\Conversion\\Facades\\Conversion"
            }
        }
    }
}
