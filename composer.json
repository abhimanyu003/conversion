{
    "name": "zdanevych/conversion",
    "description": "Laravel Unit Conversion",
     "keywords": [
         "laravel", "unit", "measurements", "conversion"
     ],
    "homepage":    "https://github.com/zdanevych/conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Abhimanyu Sharma",
            "email": "abhimanyusharma003@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0|^8.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-0": {
            "Zdanevych\\Conversion": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Zdanevych\\Conversion\\ConversionServiceProvider"
            ],
            "aliases": {
                "Conversion": "Zdanevych\\Conversion\\Facades\\Conversion"
            }
        }
    }
}
